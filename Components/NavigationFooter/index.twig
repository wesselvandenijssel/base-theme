<flynt-component name="NavigationFooter">
  <div class="container">
    {% if menu %}
      <nav class="navigation" aria-label="{{ labels.ariaLabel }}">
        {% include 'Partials/_menu.twig' with { 'items': menu.get_items, 'level': '0', 'maxLevel': maxLevel } only %}
      </nav>
    {% endif %}
    {% if contentHtml %}
      <div class="content">{{ contentHtml|e('wp_kses_post') }}</div>
    {% endif %}

    <div class="connect-items">
<i class="fa-regular fa-envelope"></i>             
<i class="fa-brands fa-linkedin-in"></i>
<i class="fa-brands fa-github"></i>                                                                
<span class="connect">connect <i class="fa-solid fa-envelope"></i></span>



    </div>
  </div>
</flynt-component>
